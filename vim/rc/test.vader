"
" Yes, we test vimscript.
"

Before (setup):
  source $HOME/.vim/rc/utility.vimrc

" StartsWith
Execute (StartsWith):
  Assert !StartsWith('abc', 'bc')
  Assert StartsWith('abc', 'ab')

" ReplaceLine
Given (hello):
  hello

Execute (SubstituteLine):
  call SubstituteLine('e', 'a', '')

Expect (hallo):
  hallo
