{
  "link": {
    "prefix": "link",
    "body": "[${1:$TM_SELECTED_TEXT}]($2)"
  },
  "sql code fence": {
    "prefix": ["`sql"],
    "body": [
      "```sql",
      "$0",
      "```"
    ]
  },
